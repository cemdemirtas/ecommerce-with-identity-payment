@model WebApplication5.Models.Entity.tbl_marka
@using WebApplication5.Models.Entity
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Create</h2>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @using (Html.BeginForm("Create", "Brand", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <table cellpadding="0" cellspacing="0">
            <tr>
                <th colspan="2" align="center">MARKA</th>
            </tr>
            <tr>
                <td>MARKA ADI: </td>
                <td>
                    @Html.TextBoxFor(m => m.markaadi)
                </td>
            </tr>

            <tr>
                <td></td>
                <td><input type="submit" value="Submit" /></td>
            </tr>
        </table>
    }
</body>
</html>